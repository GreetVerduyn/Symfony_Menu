{% extends 'base.html.twig' %}



{% block body %}
    <br>
    <div class="container">
        <h2>Edit dishes</h2>
        <br>
        <table class="table table-striped"></table>
        <thead>
        <tr>
            <th>Order Nr.</th>
            <th>Name</th>
            <th>Description</th>
            <th>Price</th>
        </tr>
        </thead>
        <tbody>
        {% for dish in dishes %}
            <tr>
                <td>{{ dish.id }}</td>
                <td>{{ dish.name }}</td>
                <td>{{ dish.description }}</td>
                <td>{{ dish.price }}</td>

            </tr>
        {% endfor %}
        </tbody>
    </div>
{% endblock %}
